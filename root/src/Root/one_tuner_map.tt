    <div class="col-xs-3">
      <!-- allows tabs on left, content is below in tab-content -->
      <div class="tabbable" id="map-select-tabs">
        <ul class="nav nav-tabs" id="tvdx-tabs">
          <li class="active">
            <a data-toggle="tab" href="#tabs-stations-rx"> Stations Received</a>
          </li>
          <li class="">
            <a data-toggle="tab" href="#tabs-rf-channel">RF Channel</a>
          </li>
        </ul>
        <div class="tab-content">
          <div class="tab-pane active" id="tabs-stations-rx">
            <div class="tabs-stations-rx-group-top" id="time-frame">
              <p>Time Frame</p>
              <div class="btn-group btn-group-xs" data-toggle="buttons">
                <label class="btn btn-primary">
                  <input name="time-frame" id="last-24-hours" type="radio"> Last 24 hours
                </label>
                <label class="btn btn-primary">
                  <input name="time-frame" id="ever" type="radio"> Ever Received
                </label>
              </div> <!-- end of Time frame btn-group btn-group-xs -->
            </div> <!-- end of time frame group -->
            <div class="tabs-stations-rx-group" id="sort-by">
              <p>Sort by</p>
              <div class="btn-group-vertical btn-group-xs" data-toggle="buttons">
                <label class="btn btn-primary">
                  <input name="sort-by" id="distance" type="radio"> Distance
                </label>
                <label class="btn btn-primary">
                  <input name="sort-by" id="rf-channel" type="radio"> RF Channel
                </label>
                <label class="btn btn-primary">
                  <input name="sort-by" id="virtual-channel" type="radio"> Virtual Channel
                </label>
                <label class="btn btn-primary">
                  <input name="sort-by" id="time-received" type="radio"> Time Received
                </label>
                <label class="btn btn-primary">
                  <input name="sort-by" id="azimuth" type="radio"> Azimuth
                </label>
                <label class="btn btn-primary">
                  <input name="sort-by" id="callsign" type="radio"> Callsign
                </label>
              </div> <!-- end of Sort by btn-group-vertical btn-group-xs -->
            </div> <!-- end of sort-by group -->
            <div class="tabs-stations-rx-group" id="distance-units">
              <p>Distance Units</p>
              <div class="btn-group btn-group-xs" data-toggle="buttons">
                <label class="btn btn-primary">
                  <input name="units" id="miles" type="radio"> Miles
                </label>
                <label class="btn btn-primary">
                  <input name="units" id="ever" type="radio"> Kilometers
                </label>
              </div> <!-- end of Distance Units btn-group btn-group-xs -->
            </div> <!-- end of Distance Units group -->
            <div class="tabs-stations-rx-group" id="stations-rx-list">
