<!-- This page is based on http://uwmike.com/maps/manhattan2/ -->
<head>
        <link rel="shortcut icon" href="[% static_url %]/favicon.ico">
	<title>TV stations received at [% tuner.owner_id %] [% tuner_number.description %]</title>
        <script type="text/javascript" src="https://www.google.com/jsapi?key=ABQIAAAA2N4H1X2kTmkGEjW247ahnxQdw6pJf8TD3LVL9vvuOZwKQe0-qxQ45rUXLLDH1quJWfw-C9I5qSbHZQ"></script>
	<script src="http://maps.googleapis.com/maps/api/js?v=3.15&sensor=false" type="text/javascript"></script>
	<script src="[% static_url %]/markerwithlabel_packed.js" type="text/javascript"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/prototype/1.7.0.0/prototype.js" type="text/javascript"></script>
	<script type="text/javascript">
tuner_id="[% tuner.tuner_id %]";
tuner_number="[% tuner_number.tuner_number %]";
// pass paths, tuner_id and tuner_number to javascript from template toolkit
root_url = '[% root_url %]'
static_url = '[% static_url %]'
        </script>
	<script src="[% static_url %]/common_map_functions.js" type="text/javascript"></script>
	<script src="[% static_url %]/one_tuner_map_functions.js" type="text/javascript"></script>
	<link href="[% static_url %]/style.css" rel="stylesheet" type="text/css" />
</head>
<body class="sidebar-right">
	<div id="toolbar">
                <!-- No page break on ownerId -->
		<h1 id="ownerId"> TV stations received at [% tuner.owner_id %] [% tuner_number.description %]
                </h1>
		<!-- Time and Date gets set by javascript -->
                <h1 id="lastUpdateTime"></h1><br>
		<img src="[% static_url %]/images/green_dot.png" width='15' height='15' />Strong signal
	        <img src="[% static_url %]/images/yellow_dot.png" width='15' height='15' />Medium signal
	        <img src="[% static_url %]/images/red_dot.png" width='15' height='15' />Weak signal
	        <img src="[% static_url %]/images/black_dot.png" width='15' height='15' />Detected in last 24 hours, but not currently<br>
                Zoom in for more stations.  Click a call sign for more details.
                This page updates every five minutes.
                Thanks to <a href="http://www.rabbitears.info">www.rabbitears.info</a> for TSID data.
	</div>
	<div id="content">
		<div id="map-wrapper">
			<div id="map"></div>
		</div>
		<div id="sidebar">
                        Station information
			<ul id="sidebar-list">
			</ul>
		</div>
	</div>
</body>
</html>
